@namespace("com.brokencircuits.kissad.messages")
protocol Simple {

  record ShowMessage {
    string title;
    union{null, int} season;
    map<string> sources;
    boolean isActive;
    union{null, string} skipEpisodeString = null;
    union{null, long} showId = null;
  }

  enum SourceName {
    HORRIBLESUBS, KISSANIME
  }
}