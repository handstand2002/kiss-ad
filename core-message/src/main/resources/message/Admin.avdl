@namespace("com.brokencircuits.messages")
protocol Simple {

  record AdminCommandKey {
    string applicationId;
    @java-class("com.brokencircuits.kissad.util.Uuid") string commandId;
  }

  enum Command {
    CHECK_NEW_EPISODES, SKIP_EPISODE_RANGE, QUERY_CAPTCHA_PICS
  }

  record AdminCommandValue {
    Command command;
    array<string> parameters;
    timestamp_ms sendTime;
  }

  record AdminCommandMsg {
    AdminCommandKey key;
    union {null, AdminCommandValue} value;
  }

}